
function run()
{
    sess=`openssl rand -base64 12`
    uid=jinru.yjr
    curl "http://11.180.61.211:34561/web" -d '{"version":1,"target":"request","communication":{"uid":"'${uid}'","sessionid":"'${sess}'","reqid":"282f7cf5-0680-446f-b8db-5cc5dfb14550","resid":0},"data":{"message":{"query":"'${1}'","status":"init"}},"bucket":{"llmrewrite":"3","llmcachewrite":"1"},"error_code":0,"error_msg":"","timestamp":"1682390791"}'
}



set -x

for q in `cat batch_query`
do
    echo "------------------------------------------------------"
    run $q
    echo
    echo
    echo
    echo
done

