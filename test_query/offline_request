sess=`openssl rand -base64 12`
reqid=`openssl rand -base64 12`

port=45671

function run()
{
    uid=jinru.yjr
    curl "http://11.180.61.211:"${port}"/offline" -d '{"version":1,"target":"request","communication":{"uid":"'${uid}'","sessionid":"'${sess}'","reqid":"'${reqid}'","resid":0},"data":{"message":{"query":"'${1}'","status":"init"}},"bucket":{"llmrewrite":"4","llmcachewrite":"1"},"error_code":0,"error_msg":"","timestamp":"1682390791"}'
}



set -x

date
