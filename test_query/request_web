sess=`openssl rand -base64 12`
reqid=`openssl rand -base64 12`

ipport=33.52.44.71:25857
ipport=11.180.61.211:56781

function run()
{
    ab=`python get_ab.py`
    echo $ab
    echo "------------------------- "$1
    uid=jinru.yjr
    curl "http://${ipport}/web" -d '{"version":1,"target":"request","communication":{"uid":"'${uid}'","sessionid":"'${sess}'","reqid":"'${reqid}'","resid":0},"data":{"message":{"query":"'${1}'","status":"init"}},"bucket":'$ab',"error_code":0,"error_msg":"","timestamp":"1682390791"}'
}



set -x

